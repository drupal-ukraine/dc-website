pipeline {
    agent {
        docker 'docksal/ci-agent:php'
    }
    environment {
        DOCKSAL_HOST='build.ivo.group'
        DOCKSAL_HOST_SSH_KEY='here_my_key'
        DOCKSAL_HOST_USER='ivo'
        GITHUB_TOKEN='git_token'
        REMOTE_BUILD_BASE='/home/berz/builds'
        CI=true
        CIRCLECI=true
        CIRCLE_BRANCH=${env.BRANCH_NAME}
    }
    stages {
        stage("Build") {
            steps {
                sh 'printenv'
            }
        }
    }
}
