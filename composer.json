    "drupal/viewsreference": "^1.2",
    "drupal/webform": "^5.2",
    "drupal/widget_engine": "^1.2",
    "drupal/workflow": "^1.1",
    "drupal/yoast_seo": "^1.3",
    "enyo/dropzone": "^5.5",
    "es6-promise": "v4.1.0",
